<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Supervisor_manageusers_drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context=".MainActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:background="@drawable/slash_background"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <!-- include your toolbar layout-->
        <TextView
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="61dp"
            android:background="@color/globankgreen"
            android:gravity="center"
            android:text="Supervisor Manage Cashier"
            android:textSize="25dp"
            android:textColor="@color/white"
            android:fontFamily="@font/nudista_bold" />
        <!-- Let's add fragment -->

        <FrameLayout
            android:id="@+id/supervisor_manageusers_frame"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/slash_background"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingTop="@dimen/activity_vertical_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:paddingBottom="@dimen/activity_vertical_margin">
            <FrameLayout
                android:id="@+id/frame"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <GridLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/gridLayout"
                    android:rowCount="4"
                    android:columnCount="3"
                    android:background="@drawable/gbkborder"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="15dp">
                <android.support.v7.widget.CardView
                    android:layout_width="130dp"
                    android:layout_height="100dp"
                    app:cardBackgroundColor="@color/blackgreen2"
                    android:layout_row="0"
                    android:layout_marginLeft="20dp"
                    android:layout_column="0"
                    android:layout_marginTop="20dp"
                    android:layout_margin="8dp"
                    android:layout_gravity="top"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_gravity="center_vertical|center_horizontal">


                <ImageButton
                    android:id="@+id/regcashier"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:background="@color/blackgreen2"
                    android:layout_gravity="center"
                    android:src="@drawable/white_add_user_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Add Cashier"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:fontFamily="@font/nudista_bold"/>
                </LinearLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:layout_width="130dp"
                    android:layout_height="100dp"
                    app:cardBackgroundColor="@color/blackgreen2"
                    android:layout_row="2"
                    android:layout_marginLeft="20dp"
                    android:layout_column="0"
                    android:layout_marginTop="20dp"
                    android:layout_margin="8dp"
                    android:layout_gravity="top"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_gravity="center_vertical|center_horizontal">

                <ImageButton
                    android:id="@+id/deletecashier"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:background="@color/blackgreen2"
                    android:layout_gravity="center"
                    android:src="@drawable/white_delete_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Delete Cashier"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textStyle="bold" />
                </LinearLayout>
                </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        android:layout_width="130dp"
                        android:layout_height="100dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="1"
                        android:layout_marginLeft="20dp"
                        android:layout_column="1"
                        android:layout_marginTop="20dp"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:gravity="center"
                            android:layout_gravity="center_vertical|center_horizontal">

                        <ImageButton
                    android:id="@+id/discashier"
                    android:layout_width="64dp"
                    android:background="@color/blackgreen2"
                    android:layout_height="64dp"
                    android:layout_gravity="center"
                    android:src="@drawable/white_disable_user_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Disable Cashier"
                                android:textAlignment="center"
                                android:textColor="@color/white"
                                android:textStyle="bold" />
                </LinearLayout>
                </android.support.v7.widget.CardView>

                    <android.support.v7.widget.CardView
                        android:layout_width="130dp"
                        android:layout_height="100dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="2"
                        android:layout_marginLeft="20dp"
                        android:layout_column="1"
                        android:layout_marginTop="20dp"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_gravity="center_vertical|center_horizontal">

                    <ImageButton
                    android:id="@+id/cangesupervisorpin"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_gravity="center"
                        android:background="@color/blackgreen2"
                    android:src="@drawable/white_pinchange_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Change My PIN"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:fontFamily="@font/nudista_bold" />
                    </LinearLayout>
                    </android.support.v7.widget.CardView>

                    <android.support.v7.widget.CardView
                        android:layout_width="130dp"
                        android:layout_height="100dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="1"
                        android:layout_marginLeft="20dp"
                        android:layout_column="0"
                        android:layout_marginTop="20dp"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_gravity="center_vertical|center_horizontal">


                    <ImageButton
                    android:id="@+id/encashier"
                    android:layout_width="64dp"
                    android:background="@color/blackgreen2"
                    android:layout_height="64dp"
                    android:layout_gravity="center"
                    android:src="@drawable/white_enable_user_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Enable Cashier"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:fontFamily="@font/nudista_bold" />
                    </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        android:layout_width="130dp"
                        android:layout_height="100dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="0"
                        android:layout_marginLeft="20dp"
                        android:layout_column="1"
                        android:layout_marginTop="20dp"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_gravity="center_vertical|center_horizontal">


                    <ImageButton
                    android:id="@+id/viewcashier"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_gravity="center"
                        android:background="@color/blackgreen2"
                    android:src="@drawable/white_view_user_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="View Cashier"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:fontFamily="@font/nudista_bold" />
                    </LinearLayout>
                    </android.support.v7.widget.CardView>

                <!--
                                             Center.....................

                      <ImageButton
                    android:id="@+id/manageusers"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_gravity="top|center"
                    android:src="@mipmap/manageusers"
                    tools:ignore="SpeakableTextPresentCheck" />


                      -->
                </GridLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_marginTop="380dp"
                    android:layout_marginLeft="70dp"
                    android:layout_height="wrap_content">
                <android.support.v7.widget.CardView
                    android:layout_width="180dp"
                    android:layout_height="90dp"
                    app:cardBackgroundColor="@color/blackgreen2"
                    android:layout_row="3"
                    android:layout_marginLeft="20dp"
                    android:layout_column="1"
                    android:layout_margin="8dp"
                    android:layout_gravity="top"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_gravity="center_vertical|center_horizontal">

                        <ImageButton
                            android:id="@+id/supervisormanagecashierhelp"
                            android:layout_width="64dp"
                            android:layout_height="64dp"
                            android:background="@color/blackgreen2"
                            android:layout_gravity="bottom|right"
                            android:src="@drawable/white_helpdesk_icon_64px"
                            tools:ignore="SpeakableTextPresentCheck" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Help"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:fontFamily="@font/nudista_bold" />
                    </LinearLayout>
                </android.support.v7.widget.CardView>
                    </LinearLayout>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
    <!--
         Navigation view to show the menu items
    -->

</android.support.v4.widget.DrawerLayout>