<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/support_drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/slash_background"

    android:fitsSystemWindows="true"
    tools:context=".MainActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <!-- include your toolbar layout-->
        <!--Button
             android:id="@+id/logout"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:text="logeout" /-->

        <android.support.v7.widget.Toolbar
            android:id="@+id/supporttoolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="@color/blackgreen2"
            android:titleTextColor="@color/globalyellow"
            >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Support Menu"
                android:textColor="@color/white"
                android:fontFamily="@font/nudista_bold"
                android:textSize="25dp"
                app:titleTextColor="#FFF" />
        </RelativeLayout>
        </android.support.v7.widget.Toolbar>

        <TextView
            android:id="@+id/logoon"
            android:layout_width="310dp"
            android:layout_height="30dp"
            android:layout_gravity="center"
            android:layout_marginTop="2dp"
            android:background="@drawable/slash_background"
            android:gravity="center"
            android:text="Logged In Support:  "
            android:fontFamily="@font/panton_bold"
            android:textColor="@color/blackgreen2"
            android:textSize="23dp" />
        <!-- Let's add fragment -->

        <FrameLayout

            android:id="@+id/support_frame"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/slash_background"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingBottom="@dimen/activity_vertical_margin">
            <FrameLayout
                android:id="@+id/frame"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <GridLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/gridLayout"
                    android:rowCount="4"
                    android:columnCount="3"
                    android:background="@drawable/gbkborder"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="15dp">
                <android.support.v7.widget.CardView
                    android:layout_width="130dp"
                    android:layout_height="100dp"
                    app:cardBackgroundColor="@color/blackgreen2"
                    android:layout_row="0"
                    android:layout_marginLeft="20dp"
                    android:layout_column="0"
                    android:layout_marginTop="20dp"
                    android:layout_margin="8dp"
                    android:layout_gravity="top"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_gravity="center_vertical|center_horizontal">


                <ImageButton
                    android:id="@+id/comconfig"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_gravity="center"
                    android:background="@color/blackgreen2"
                    android:src="@drawable/white_com_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Communication Config"
                        android:textAlignment="center"
                        android:fontFamily="@font/nudista_bold"
                        android:textColor="@color/white"
                         />
                </LinearLayout>
                </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        android:layout_width="130dp"
                        android:layout_height="100dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="0"
                        android:layout_marginLeft="20dp"
                        android:layout_column="1"
                        android:layout_marginTop="20dp"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_gravity="center_vertical|center_horizontal">

                    <ImageButton
                    android:id="@+id/termconfig"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_gravity="center"
                        android:background="@color/blackgreen2"
                    android:src="@drawable/white_pos4_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Terminal Config"
                            android:textAlignment="center"
                            android:fontFamily="@font/nudista_bold"
                            android:textColor="@color/white"
                            />
                    </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        android:layout_width="130dp"
                        android:layout_height="100dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="1"
                        android:layout_marginLeft="20dp"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_gravity="center_vertical|center_horizontal">

                <ImageButton
                    android:id="@+id/viewterminfo"
                    android:layout_width="64dp"
                    android:background="@color/blackgreen2"
                    android:layout_height="64dp"
                    android:layout_gravity="center"
                    android:src="@drawable/white_com_reprint_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="View Terminal Info"
                            android:textAlignment="center"
                            android:fontFamily="@font/nudista_bold"
                            android:textColor="@color/white"
                            />
                    </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        android:layout_width="130dp"
                        android:layout_height="100dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="1"
                        android:layout_marginLeft="20dp"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_gravity="center_vertical|center_horizontal">

                    <ImageButton
                    android:id="@+id/comsetupreprint"
                    android:layout_width="64dp"
                        android:background="@color/blackgreen2"
                    android:layout_height="64dp"
                    android:layout_gravity="center"
                    android:src="@drawable/white_reprint_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Com Setup Reprint"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:fontFamily="@font/nudista_bold" />
                    </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        android:layout_width="130dp"
                        android:layout_height="100dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="2"
                        android:layout_marginLeft="20dp"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_gravity="center_vertical|center_horizontal">

                    <ImageButton
                    android:id="@+id/termsetupreprint"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                        android:background="@color/blackgreen2"
                    android:layout_gravity="center"
                    android:src="@drawable/white_pos1_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Terminal Setup Reprint"
                            android:textAlignment="center"
                            android:fontFamily="@font/nudista_bold"
                            android:textColor="@color/white"
                            />
                    </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        android:layout_width="130dp"
                        android:layout_height="100dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="2"
                        android:layout_marginLeft="20dp"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_gravity="center_vertical|center_horizontal">


                    <ImageButton
                    android:id="@+id/settings"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                        android:background="@color/blackgreen2"
                    android:layout_gravity="center"
                    android:src="@drawable/white_settings_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Settings"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:fontFamily="@font/nudista_bold"
                             />
                    </LinearLayout>
                    </android.support.v7.widget.CardView>
                <!--
                         Horizontal.....................
                    -->

                <!--
                                         bottom.....................
                  -->

                </GridLayout>


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_marginTop="360dp"
                    android:layout_marginLeft="80dp"
                    android:layout_height="wrap_content">
                    <android.support.v7.widget.CardView
                        android:layout_width="160dp"
                        android:layout_height="90dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="3"
                        android:layout_marginLeft="20dp"
                        android:layout_column="1"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:gravity="center"
                            android:layout_gravity="center_vertical|center_horizontal">

                            <ImageButton
                                android:id="@+id/supportmenuhelp"
                                android:layout_width="64dp"
                                android:layout_height="64dp"
                                android:background="@color/blackgreen2"
                                android:layout_gravity="bottom|right"
                                android:src="@drawable/white_helpdesk_icon_64px"
                                tools:ignore="SpeakableTextPresentCheck" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Help"
                                android:textAlignment="center"
                                android:fontFamily="@font/nudista_bold"
                                android:textColor="@color/white"
                                />
                        </LinearLayout>
                    </android.support.v7.widget.CardView>
                </LinearLayout>
            </FrameLayout>
        </FrameLayout>
    </LinearLayout>
    <!--
         Navigation view to show the menu items
    -->


    <android.support.design.widget.NavigationView
        android:id="@+id/supportnavigation_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        app:headerLayout="@layout/nav_header"
        app:menu="@menu/nav_menu"
        app:itemTextColor="@color/blackgreen2"
        app:iconTint="@color/blackgreen2"
        app:theme="@style/AppTheme.NavigationView"
        android:background="@drawable/slash_background2" />


</android.support.v4.widget.DrawerLayout>