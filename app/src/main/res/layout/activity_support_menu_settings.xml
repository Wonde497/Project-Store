<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/support_settings_drawer_layout"
    android:layout_width="match_parent"
    android:background="@drawable/slash_background"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context=".MainActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/slash_background"

        android:orientation="vertical">
        <!-- include your toolbar layout-->
        <TextView
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="61dp"
            android:textSize="25dp"
            android:textColor="@color/white"
            android:gravity="center"
            android:fontFamily="@font/nudista_bold"
            android:text="Support Settings Menu"
            android:background="@color/blackgreen2"
            />
        <!-- Let's add fragment -->

        <FrameLayout
            android:id="@+id/support_settings_frame"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/slash_background"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingBottom="@dimen/activity_vertical_margin">
            <FrameLayout
                android:id="@+id/frame"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <GridLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/gridLayout"
                    android:rowCount="5"
                    android:columnCount="3"
                    android:background="@drawable/gbkborder"
                    android:layout_centerHorizontal="true"
                    android:layout_marginBottom="8dp">
                <android.support.v7.widget.CardView
                    android:layout_width="130dp"
                    android:layout_height="100dp"
                    app:cardBackgroundColor="@color/blackgreen2"
                    android:layout_row="2"
                    android:layout_marginLeft="20dp"
                    android:layout_column="1"
                    android:layout_marginTop="10dp"
                    android:layout_margin="8dp"
                    android:layout_gravity="top"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="10dp">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_gravity="center_vertical|center_horizontal">


                <ImageButton
                    android:id="@+id/gallery"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_gravity="center"
                    android:background="@color/blackgreen2"
                    android:src="@drawable/white_logo_change_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Change Logo"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:fontFamily="@font/nudista_bold" />
                </LinearLayout>
                </android.support.v7.widget.CardView>

                    <android.support.v7.widget.CardView
                        android:layout_width="130dp"
                        android:layout_height="100dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="0"
                        android:layout_marginLeft="20dp"
                        android:layout_column="0"
                        android:layout_marginTop="20dp"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_gravity="center_vertical|center_horizontal">

                <ImageButton
                    android:id="@+id/regsupervisor"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_gravity="center"
                    android:background="@color/blackgreen2"
                    android:src="@drawable/white_add_user_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Add Supervisor"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:fontFamily="@font/nudista_bold" />
                    </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        android:layout_width="130dp"
                        android:layout_height="100dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="0"
                        android:layout_marginLeft="20dp"
                        android:layout_column="1"
                        android:layout_marginTop="20dp"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_gravity="center_vertical|center_horizontal">

                <ImageButton
                    android:id="@+id/manageusers"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_gravity="center"
                    android:background="@color/blackgreen2"
                    android:src="@drawable/white_manageuser_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Manage Supervisor"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:fontFamily="@font/nudista_bold"/>
                    </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        android:layout_width="130dp"
                        android:layout_height="100dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="1"
                        android:layout_marginLeft="20dp"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_gravity="center_vertical|center_horizontal">


                <ImageButton
                    android:id="@+id/comsetup"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_gravity="center"
                    android:background="@color/blackgreen2"
                    android:src="@drawable/white_com_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Communication Setup"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:fontFamily="@font/nudista_bold" />
                    </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        android:layout_width="130dp"
                        android:layout_height="100dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="1"
                        android:layout_marginLeft="20dp"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_gravity="center_vertical|center_horizontal">


                    <ImageButton
                    android:id="@+id/cashierlog"
                    android:layout_width="64dp"
                    android:layout_height="60dp"
                    android:layout_gravity="center"
                        android:background="@color/blackgreen2"
                    android:src="@drawable/white_off_icon2_64px"
                    tools:ignore="SpeakableTextPresentCheck" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Cashier Login"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:fontFamily="@font/nudista_bold" />
                    </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        android:layout_width="130dp"
                        android:layout_height="100dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="2"
                        android:layout_marginLeft="20dp"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:gravity="center"
                        android:layout_gravity="center_vertical|center_horizontal">


                    <ImageButton
                    android:id="@+id/managetrans"
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:layout_gravity="center"
                        android:background="@color/blackgreen2"
                    android:src="@drawable/white_manage_transaction_icon_64px"
                    tools:ignore="SpeakableTextPresentCheck" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Manage\nTransaction"
                            android:textAlignment="center"
                            android:textColor="@color/white"
                            android:fontFamily="@font/nudista_bold" />
                    </LinearLayout>
                    </android.support.v7.widget.CardView>


                    <android.support.v7.widget.CardView
                        android:layout_width="130dp"
                        android:layout_height="100dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="4"
                        android:layout_marginLeft="20dp"
                        android:layout_column="0"
                        android:layout_marginTop="10dp"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:gravity="center"
                            android:layout_gravity="center_vertical|center_horizontal">


                            <ImageButton
                                android:id="@+id/auto_settlement"
                                android:layout_width="64dp"
                                android:layout_height="60dp"
                                android:layout_gravity="center"
                                android:background="@color/blackgreen2"
                                android:src="@drawable/white_off_icon2_64px"
                                tools:ignore="SpeakableTextPresentCheck" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Auto Settlement"
                                android:textAlignment="center"
                                android:textColor="@color/white"
                                android:fontFamily="@font/nudista_bold" />
                        </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        android:layout_width="130dp"
                        android:layout_height="100dp"
                        app:cardBackgroundColor="@color/blackgreen2"
                        android:layout_row="4"
                        android:layout_marginLeft="20dp"
                        android:layout_column="1"
                        android:layout_marginTop="10dp"
                        android:layout_margin="8dp"
                        android:layout_gravity="top"
                        app:cardCornerRadius="20dp"
                        app:cardElevation="10dp">
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:gravity="center"
                            android:layout_gravity="center_vertical|center_horizontal">


                            <ImageButton
                                android:id="@+id/settlement_time"
                                android:layout_width="64dp"
                                android:layout_height="64dp"
                                android:layout_gravity="center"
                                android:background="@color/blackgreen2"
                                android:src="@drawable/settlement_time"
                                tools:ignore="SpeakableTextPresentCheck" />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Settlement Time"
                                android:textAlignment="center"
                                android:textColor="@color/white"
                                android:fontFamily="@font/nudista_bold" />
                        </LinearLayout>
                    </android.support.v7.widget.CardView>



                    <!--
                                                 Center.....................

                          <ImageButton
                        android:id="@+id/manageusers"
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:layout_gravity="top|center"
                        android:src="@mipmap/manageusers"
                        tools:ignore="SpeakableTextPresentCheck" />


                          -->
                </GridLayout>
